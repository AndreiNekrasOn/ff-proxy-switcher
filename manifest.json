{
  "manifest_version": 2,
  "name": "Quick Proxy Switcher",
  "version": "1.0",
  "homepage_url": "https://github.com/AndreiNekrasOn/ff-proxy-switcher",
  "description": "Switch proxy without opening about:preferences",
  "permissions": ["proxy", "storage"],
  "icons": {
    "48": "icons/border-48.png"
  },
  "browser_action": {
    "default_icon": "icons/border-48.png",
    "default_popup": "popup.html"
  },
  "options_ui": {
    "page": "options/preferences.html"
  }
}
